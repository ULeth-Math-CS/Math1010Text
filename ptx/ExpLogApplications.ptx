    <exercise xml:id="radioactivelast">
      <statement>
        <p>
          Uranium 235, used for nuclear power,
          initial amount <m>1</m> kg grams,
          half-life <m>704</m> million years.
        </p>
      </statement>
      <answer>
        <ul>
          <li>
            <p>
              <m>k = \frac{\ln(1/2)}{704} \approx -0.0010</m>
            </p>
          </li>

          <li>
            <p>
              <m>A(t) = e^{-0.0010t}</m>
            </p>
          </li>

